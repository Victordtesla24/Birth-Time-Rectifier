{
  "name": "birth-time-rectifier",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:ci": "jest --ci --coverage && playwright test",
    "test:update-snapshots": "jest -u",
    "test:performance": "jest --testMatch='**/*.perf.test.(ts|tsx)' --runInBand",
    "test:accessibility": "jest --testMatch='**/*.a11y.test.(ts|tsx)'",
    "test:hooks": "jest --testMatch='**/hooks/**/*.test.(ts|tsx)'",
    "test:integration": "jest --testMatch='**/integration/**/*.test.(ts|tsx)'",
    "test:visual": "playwright test --config=playwright.config.ts --project=visual-regression",
    "test:memory": "jest --testMatch='**/*.memory.test.(ts|tsx)' --runInBand",
    "test:framerate": "jest --testMatch='**/*.framerate.test.(ts|tsx)' --runInBand",
    "test:rendertime": "jest --testMatch='**/*.rendertime.test.(ts|tsx)' --runInBand",
    "test:aggregate": "node scripts/aggregate-test-results.js",
    "pretest:e2e": "npm run build",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}'",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run typecheck && npm run test"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.16.14",
    "@mui/material": "^5.16.14",
    "axios": "^1.6.7",
    "d3": "^7.9.0",
    "formik": "^2.4.6",
    "framer-motion": "^11.0.3",
    "gsap": "^3.12.7",
    "lodash": "^4.17.21",
    "next": "14.1.0",
    "p5": "^1.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.2.4",
    "tweakpane": "^4.0.5",
    "yup": "^1.6.1",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/plugin-transform-runtime": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@mui/x-date-pickers": "^7.26.0",
    "@playwright/test": "^1.50.1",
    "@swc/jest": "^0.2.37",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3": "^7.4.3",
    "@types/jest": "^29.5.14",
    "@types/jest-axe": "^3.5.9",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.5",
    "@types/p5": "^1.7.6",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "date-fns": "^4.1.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "history": "^5.3.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "jest-image-snapshot": "^6.4.0",
    "jest-junit": "^16.0.0",
    "jest-performance": "^1.0.0",
    "jest-playwright-preset": "^3.0.1",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.2.0",
    "msw": "^2.7.0",
    "prettier": "^3.1.1",
    "react-app-polyfill": "^3.0.0",
    "react-router-dom": "^6.29.0",
    "start-server-and-test": "^2.0.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.3.3",
    "whatwg-fetch": "^3.6.20"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/testSetup.tsx"
    ]
  }
}
